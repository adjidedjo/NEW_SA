<h3>Elite <%= @region %></h3>
<!-- START carousel-->
<div id="carousel-example-captions" data-ride="carousel" class="carousel slide">
  <ol class="carousel-indicators">
    <li data-target="#carousel-example-captions" data-slide-to="0" class="active"></li>
    <li data-target="#carousel-example-captions" data-slide-to="1"></li>
    <li data-target="#carousel-example-captions" data-slide-to="2"></li>
    <li data-target="#carousel-example-captions" data-slide-to="3"></li>
    <li data-target="#carousel-example-captions" data-slide-to="4"></li>
    <li data-target="#carousel-example-captions" data-slide-to="5"></li>
    <li data-target="#carousel-example-captions" data-slide-to="6"></li>
    <li data-target="#carousel-example-captions" data-slide-to="7"></li>
  </ol>
  <div role="listbox" class="carousel-inner">
    <div class="item active">
      <%= link_to image_tag("#{@brand_name} p1.jpg", alt: "elite luxury series"), image_path("#{@brand_name} p1.jpg") %>
      <div class="carousel-caption">
        <h3>LUXURY SERIES</h3>
        <p>
          Elite <%= @region %>.
        </p>
      </div>
    </div>
    <div class="item">
      <%= link_to image_tag("#{@brand_name} p2.jpg", alt: "elite ruby series"), image_path("#{@brand_name} p2.jpg") %>
      <div class="carousel-caption">
        <h3>RUBY SERIES</h3>
        <p>
          Elite <%= @region %>.
        </p>
      </div>
    </div>
    <div class="item">
      <%= link_to image_tag("#{@brand_name} p3.jpg", alt: "elite emerald series"), image_path("#{@brand_name} p3.jpg") %>
      <div class="carousel-caption">
        <h3>EMERALD SERIES</h3>
        <p>
          Elite <%= @region %>.
        </p>
      </div>
    </div>
    <div class="item">
      <%= link_to image_tag("#{@brand_name} p4.jpg", alt: "elite sapphire series"), image_path("#{@brand_name} p4.jpg") %>
      <div class="carousel-caption">
        <h3>SAPPHIRE SERIES</h3>
        <p>
          Elite <%= @region %>.
        </p>
      </div>
    </div>
    <div class="item">
      <%= link_to image_tag("#{@brand_name} p5.jpg", alt: "elite orthopedic series"), image_path("#{@brand_name} p5.jpg") %>
      <div class="carousel-caption">
        <h3>ORTHOPEDIC SERIES</h3>
        <p>
          Elite <%= @region %>.
        </p>
      </div>
    </div>
    <div class="item">
      <%= link_to image_tag("#{@brand_name} p6.jpg", alt: "elite family series"), image_path("#{@brand_name} p6.jpg") %>
      <div class="carousel-caption">
        <h3>FAMILY SERIES</h3>
        <p>
          Elite <%= @region %>.
        </p>
      </div>
    </div>
    <div class="item">
      <%= link_to image_tag("#{@brand_name} p7.jpg", alt: "elite orthopedic series"), image_path("#{@brand_name} p7.jpg") %>
      <div class="carousel-caption">
        <h3>KIDS SERIES</h3>
        <p>
          Elite <%= @region %>.
        </p>
      </div>
    </div>
    <div class="item">
      <%= link_to image_tag("#{@brand_name} p8.jpg", alt: "elite accesories series"), image_path("#{@brand_name} p8.jpg") %>
      <div class="carousel-caption">
        <h3>ACCESORIES</h3>
        <p>
          Elite <%= @region %>.
        </p>
      </div>
    </div>
  </div>
  <a href="#carousel-example-captions" role="button" data-slide="prev" class="left carousel-control"> <span aria-hidden="true" class="glyphicon glyphicon-chevron-left"></span> <span class="sr-only">Previous</span> </a>
  <a href="#carousel-example-captions" role="button" data-slide="next" class="right carousel-control"> <span aria-hidden="true" class="glyphicon glyphicon-chevron-right"></span> <span class="sr-only">Next</span> </a>
</div>
<!-- END carousel-->

<div id="popup"></div>

<script type="text/javascript">
    $(document).ready(function() {
        $(".popup_link").click(function(e) {
            e.preventDefault();
            $("#popup").html($('<img>').attr('src', this.href));
        });
    }); 
</script>