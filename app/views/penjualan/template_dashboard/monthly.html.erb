<div class="content-heading">
  Dashboard <%= @brand_name %> <%= 1.month.ago.strftime("%B %Y") %> REPORT for <%= @branch %> BRANCH
</div>
<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START chart-->
    <div class="row">
      <div class="col-lg-12">
        <!-- START widget-->
        <div id="panelChart9" class="panel panel-default panel-demo">
          <div class="panel-heading">
            <div class="panel-title" style="text-align: center">
              <%= @brand_name %> sales <%= 4.month.ago.strftime("%B %Y") %> - <%= 1.month.ago.strftime("%B %Y") %> sales statistics
            </div>
          </div>
          <div class="panel-body">
            <div class="chart-splinev2 flot-chart"></div>
          </div>
        </div>
        <!-- END widget-->
      </div>
    </div>
    <!-- END chart-->
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            <%= @brand_name %> sales <%= 1.month.ago.to_date.strftime("%B %Y") %> by Month
          </div>
          <div class="panel-body">
            <table id="datatable1" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Bulan</th>
                  <th class="sort-numeric">Jumlah</th>
                  <th class="sort-alpha">Total</th>
                </tr>
              </thead>
              <tbody>
                <% @all_month_brand.each do |cs| %>
                <tr class="gradeX">
                  <td><%= cs.tanggalsj.strftime("%B") %></td>
                  <td><%= cs.jumlah %></td>
                  <td><%= currency(cs.price) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            <%= @brand_name %> sales <%= 1.month.ago.to_date.strftime("%B %Y") %> by Product
          </div>
          <div class="panel-body">
            <table id="datatable_product" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Product</th>
                  <th>Q_M(<%= 2.month.ago.month %>/<%= 2.month.ago.strftime("%y") %>)</th>
                  <th>V_M(<%= 2.month.ago.month %>/<%= 2.month.ago.strftime("%y") %>)</th>
                  <th>Q_M(<%= 1.month.ago.month %>/<%= 1.month.ago.strftime("%y") %>)</th>
                  <th>V_M(<%= 1.month.ago.month %>/<%= 1.month.ago.strftime("%y") %>)</th>
                </tr>
              </thead>
              <tbody>
                <% @product_month_summary.each do |cws| %>
                <tr class="gradeX">
                  <td><%= cws.kodejenis %></td>
                  <td><%= cws.q_two %></td>
                  <td><%= currency(cws.v_two) %></td>
                  <td><%= cws.q_one %></td>
                  <td><%= currency(cws.v_one) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- END dashboard main content-->
  <div class="col-lg-12">
    <!-- START CONTENT-->
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            <%= @brand_name %> sales <%= 1.month.ago.to_date.strftime("%B %Y") %> by City
          </div>
          <div class="panel-body">
            <table id="datatable_city" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>City</th>
                  <th>Q_M(<%= 2.month.ago.month %>/<%= 2.month.ago.strftime("%y") %>)</th>
                  <th>V_M(<%= 2.month.ago.month %>/<%= 2.month.ago.strftime("%y") %>)</th>
                  <th>Q_M(<%= 1.month.ago.month %>/<%= 1.month.ago.strftime("%y") %>)</th>
                  <th>V_M(<%= 1.month.ago.month %>/<%= 1.month.ago.strftime("%y") %>)</th>
                </tr>
              </thead>
              <tbody>
                <% @city_month_sum.each do |cws| %>
                <tr class="gradeX">
                  <td><%= cws.kota %></td>
                  <td><%= cws.q_two %></td>
                  <td><%= currency(cws.v_two) %></td>
                  <td><%= cws.q_one %></td>
                  <td><%= currency(cws.v_one) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            <%= @brand_name %> sales <%= 1.month.ago.to_date.strftime("%B %Y") %> by Customer
          </div>
          <div class="panel-body">
            <table id="datatable_customer" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Customer</th>
                  <th>Salesman</th>
                  <th class="sort-numeric">Jumlah</th>
                  <th class="sort-alpha">Total Harga</th>
                </tr>
              </thead>
              <tbody>
                <% @customer_summary.each do |cs| %>
                <tr class="gradeX">
                  <td><%= cs.customer %></td>
                  <td><%= cs.salesman %></td>
                  <td><%= cs.ordered %></td>
                  <td><%= currency(cs.price) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-12">
    <!-- START CONTENT-->
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            <%= @brand_name %> sales Week(<%= 1.week.ago.to_date.cweek %>) by Top 10
          </div>
          <div class="panel-body">
            <table id="most_item" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Article</th>
                  <th>Size</th>
                  <th>Qty</th>
                  <th>Value</th>
                </tr>
              </thead>
              <tbody>
                <% @most_item.each do |cws| %>
                <tr class="gradeX">
                  <td><%= cws.namaartikel %></td>
                  <td><%= cws.lebar %></td>
                  <td><%= cws.ordered %></td>
                  <td><%= currency(cws.value) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>