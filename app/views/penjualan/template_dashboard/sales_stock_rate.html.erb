<div class="content-heading">
  Dashboard <%= @brand_name %> <%= 1.month.ago.strftime("%B %Y") %> REPORT for <%= @branch %> BRANCH
</div>
<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START chart-->
    <div class="row">
      <div class="col-lg-12">
        <!-- START widget-->
        <div id="panelChart9" class="panel panel-default panel-demo">
          <div class="panel-heading">
            <div class="panel-title" style="text-align: center">
              <%= @brand_name %> <%= 1.month.ago.strftime("%Y") %> sales statistics
            </div>
          </div>
          <div class="panel-body">
            <div class="chart-bar-<%= @brand_name %> flot-chart"></div>
          </div>
        </div>
        <!-- END widget-->
      </div>
    </div>
    <!-- END chart-->
  </div>
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-warning">
          <div class="panel-heading" style="text-align: center">
            Total Revenue <%= Date.yesterday.last_month.strftime("%B %Y") %>
          </div>
          <div class="panel-body">
            <table id="table-revenue" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th><%= Date.yesterday.last_month.strftime('%b %y') %></th>
                  <th><%= 2.month.ago.strftime('%b %y') %></th>
                  <th>Change</th>
                  <th><%= Date.yesterday.last_month.last_year.strftime('%b %y') %></th>
                  <th>YoY</th>
                </tr>
              </thead>
              <tbody>
                <% @sales_revenue_last_month.each do |dp| %>
                <tr class="gradeX">
                  <td><%= currency(dp.val_1) %></td>
                  <td><%= currency(dp.val_2) %></td>
                  <td class="<%= coloring(dp.percentage) %>"><%= dp.percentage.nil? ? 0 : cpercent(dp.percentage) %></td>
                  <td><%= currency(dp.revenue) %></td>
                  <td class="<%= coloring(dp.y_percentage) %>"><%= dp.y_percentage.nil? ? 0 : cpercent(dp.y_percentage) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            <%= @brand_name %> sales <%= 1.month.ago.to_date.strftime("%B %Y") %> by Product
          </div>
          <div class="panel-body">
            <table id="datatable_product" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Product</th>
                  <th><%= Date.yesterday.last_month.strftime('%b %y') %></th>
                  <th><%= Date.yesterday.last_month.strftime('%b %y') %></th>
                  <th>MoM</th>
                  <th>Target</th>
                </tr>
              </thead>
              <tbody>
                <% @product_month_summary.each do |cws| %>
                <tr class="gradeX">
                  <td><%= cws.kodejenis %></td>
                  <td><%= cws.qty_1 %></td>
                  <td><%= currency(cws.val_1) %></td>
                  <td class="<%= coloring(cws.percentage) %>"><%= cws.percentage.nil? ? 0 : cpercent(cws.percentage) %></td>
                  <td class="<%= coloring_target(cws.target) %>"><%= cws.target.nil? ? 0 : cpercent(cws.target) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
    <!-- END content-->
  </div>
  <div class="col-lg-12">
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            <%= @brand_name %> sales <%= 1.month.ago.to_date.strftime("%B %Y") %> by Customer
          </div>
          <div class="panel-body">
            <table id="datatable_customer" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Customer</th>
                  <th>KM</th>
                  <th>DV</th>
                  <th>HB</th>
                  <th>SA</th>
                  <th>SB</th>
                  <th>KB</th>
                  <th>Total</th>
                  <th>MoM</th>
                </tr>
              </thead>
              <tbody>
                <% @last_month_customer.each do |dp| %>
                <tr class="gradeX">
                  <td><%= dp.customer %></td>
                  <td><%= dp.km %></td>
                  <td><%= dp.dv %></td>
                  <td><%= dp.hb %></td>
                  <td><%= dp.sa %></td>
                  <td><%= dp.sb %></td>
                  <td><%= dp.kb %></td>
                  <td><%= currency(dp.total_1) %></td>
                  <td class="<%= coloring(dp.percentage) %>"><%= dp.percentage.nil? ? 0 : cpercent(dp.percentage) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            <%= @brand_name %> sales <%= 1.month.ago.to_date.strftime("%B %Y") %> by City
          </div>
          <div class="panel-body">
            <table id="datatable_city" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>City</th>
                  <th>KM</th>
                  <th>DV</th>
                  <th>HB</th>
                  <th>SA</th>
                  <th>SB</th>
                  <th>KB</th>
                  <th>Total</th>
                  <th>MoM</th>
                </tr>
              </thead>
              <tbody>
                <% @city_month_sum.each do |dp| %>
                <tr class="gradeX">
                  <td><%= dp.kota %></td>
                  <td><%= dp.km %></td>
                  <td><%= dp.dv %></td>
                  <td><%= dp.hb %></td>
                  <td><%= dp.sa %></td>
                  <td><%= dp.sb %></td>
                  <td><%= dp.kb %></td>
                  <td><%= currency(dp.total_1) %></td>
                  <td class="<%= coloring(dp.percentage) %>"><%= dp.percentage.nil? ? 0 : cpercent(dp.percentage) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-12">
    <!-- START CONTENT-->
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            <%= @brand_name %> sales <%= 1.month.ago.to_date.strftime("%B %Y") %> by Article
          </div>
          <div class="panel-body">
            <table id="most_item" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Article</th>
                  <th>Size</th>
                  <th><%= Date.yesterday.last_month.strftime('%b %y') %></th>
                  <th><%= Date.yesterday.last_month.strftime('%b %y') %></th>
                  <th>MoM</th>
                </tr>
              </thead>
              <tbody>
                <% @last_month_article.each do |dp| %>
                <tr class="gradeX">
                  <td><%= dp.namaartikel %></td>
                  <td><%= dp.lebar %></td>
                  <th><%= dp.qty_1 %></th>
                  <th><%= currency(dp.val_1) %></th>
                  <td class="<%= coloring(dp.percentage) %>"><%= dp.percentage.nil? ? 0 : cpercent(dp.percentage) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>