<div class="content-heading">
  Dashboard NASIONAL <%= @brand_name %> From 4 Weeks Ago to Last Week
</div>
<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START chart-->
    <div class="row">
      <div class="col-lg-12">
        <!-- START widget-->
        <div id="panelChart9" class="panel panel-default panel-demo">
          <div class="panel-heading">
            <div class="panel-title" style="text-align: center">
              Nasional <%= @brand_name %> Monthly sales statistics <br /> (x10.000.000)
            </div>
          </div>
          <div class="panel-body">
            <div class="chart-line-monthly flot-chart"></div>
          </div>
        </div>
        <!-- END widget-->
      </div>
    </div>
  </div>
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START chart-->
    <div class="row">
      <div class="col-lg-12">
        <!-- START widget-->
        <div id="panelChart9" class="panel panel-default panel-demo">
          <div class="panel-heading">
            <div class="panel-title" style="text-align: center">
              Nasional <%= @brand_name %> <b><%= Date.yesterday.last_month.strftime('%B %Y') %></b> sales statistics by Branch<br /> (x10.000.000)
            </div>
          </div>
          <div class="panel-body">
            <div class="chart-line-monthlynas flot-chart"></div>
          </div>
        </div>
        <!-- END widget-->
      </div>
    </div>
  </div>
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START chart-->
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            <%= @brand_name %> sales <%= 1.month.ago.to_date.strftime("%B %Y") %> by Product
          </div>
          <div class="panel-body">
            <table id="datatable_product" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Product</th>
                  <th><%= Date.yesterday.last_month.strftime('%b %y') %></th>
                  <th><%= Date.yesterday.last_month.strftime('%b %y') %></th>
                  <th>MoM</th>
                  <th>Target</th>
                </tr>
              </thead>
              <tbody>
                <% @product_monthnas_summary.each do |cws| %>
                <tr class="gradeX">
                  <td><%= cws.kodejenis %></td>
                  <td><%= cws.qty_1 %></td>
                  <td><%= currency(cws.val_1) %></td>
                  <td class="<%= coloring(cws.percentage) %>"><%= cws.percentage.nil? ? 0 : cpercent(cws.percentage) %></td>
                  <td class="<%= coloring_target(cws.target) %>"><%= cws.target.nil? ? 0 : cpercent(cws.target) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>