<div class="content-heading">
  Dashboard NASIONAL <%= @brand_name %> From <b><%= Date.yesterday.beginning_of_year.strftime('%B %Y') %></b> To <b><%= Date.yesterday.strftime('%B %Y') %></b>
</div>
<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START chart-->
    <div class="row">
      <div class="col-lg-12">
        <!-- START widget-->
        <div id="panelChart9" class="panel panel-default panel-demo">
          <div class="panel-heading">
            <div class="panel-title" style="text-align: center">
              Nasional <%= @brand_name %> <b><%= Date.yesterday.beginning_of_month.strftime('%d') %>-<%= Date.yesterday.strftime('%d %B %Y') %></b> sales statistics by Branch
              <br />
              (x10.000.000)
            </div>
          </div>
          <div class="panel-body">
            <div class="chart-bar-monthlynas flot-chart"></div>
          </div>
        </div>
        <!-- END widget-->
      </div>
    </div>
  </div>
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-12">
        <div class="panel panel-warning">
          <div class="panel-heading" style="text-align: center">
            Total Revenue <%= Date.yesterday.beginning_of_month.strftime('%d') %>-<%= Date.yesterday.strftime('%d %B %Y') %>
          </div>
          <div class="panel-body">
            <table id="datatable1" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th><%= Date.yesterday.strftime('%b %y') %></th>
                  <th><%= 1.month.ago.strftime('%b %y') %></th>
                  <th>Change</th>
                  <th><%= Date.yesterday.last_year.strftime('%b %y') %></th>
                  <th>YoY</th>
                  <th>Target Amount</th>
                  <th>Target <%= Date.yesterday.strftime('%b %y') %></th>
                  <th>Target to Year</th>
                </tr>
              </thead>
              <tbody>
                <% @monthnas_summary.each do |dp| %>
                <tr class="gradeX">
                  <td><%= currency(dp.val_1) %></td>
                  <td><%= currency(dp.val_2) %></td>
                  <td class="<%= coloring(dp.percentage) %>"><%= dp.percentage.nil? ? 0 : cpercent(dp.percentage) %></td>
                  <td><%= currency(dp.revenue) %></td>
                  <td class="<%= coloring(dp.y_percentage) %>"><%= dp.y_percentage.nil? ? 0 : cpercent(dp.y_percentage) %></td>
                  <td><%= currency(dp.month_target) %></td>
                  <td class="<%= coloring(dp.t_percentage) %>"><%= dp.t_percentage.nil? ? 0 : cpercent(dp.t_percentage) %></td>
                  <td><%= dp.ty_percentage.nil? ? 0 : cpercent(dp.ty_percentage) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
    <!-- END content-->
  </div>
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-12">
        <div class="panel panel-info">
          <div class="panel-heading" style="text-align: center">
            Total Revenue <%= Date.yesterday.beginning_of_month.strftime('%d') %>-<%= Date.yesterday.strftime('%d %B %Y') %> By Branches
          </div>
          <div class="panel-body">
            <table id="table-revenue" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Branch</th>
                  <th><%= Date.yesterday.strftime('%b %y') %></th>
                  <th><%= 1.month.ago.strftime('%b %y') %></th>
                  <th>Change</th>
                  <th><%= Date.yesterday.last_year.strftime('%b %y') %></th>
                  <th>YoY</th>
                  <th>Target Amount</th>
                  <th>Target <%= Date.yesterday.strftime('%b %y') %></th>
                  <th>Target to Year</th>
                </tr>
              </thead>
              <tbody>
                <% @branches_monthnas_summary.each do |dp| %>
                <tr class="gradeX">
                  <td><%= dp.cabang.gsub('Cabang', '') unless dp.cabang.nil? %></td>
                  <td><%= currency(dp.val_1) %></td>
                  <td><%= currency(dp.val_2) %></td>
                  <td class="<%= coloring(dp.percentage) %>"><%= dp.percentage.nil? ? 0 : cpercent(dp.percentage) %></td>
                  <td><%= currency(dp.revenue) %></td>
                  <td class="<%= coloring(dp.y_percentage) %>"><%= dp.y_percentage.nil? ? 0 : cpercent(dp.y_percentage) %></td>
                  <td><%= currency(dp.month_target) %></td>
                  <td class="<%= coloring(dp.t_percentage) %>"><%= dp.t_percentage.nil? ? 0 : cpercent(dp.t_percentage) %></td>
                  <td><%= dp.ty_percentage.nil? ? 0 : cpercent(dp.ty_percentage) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
    <!-- END content-->
  </div>
</div>