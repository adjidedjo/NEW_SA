<div class="content-heading">
  Dashboard <%= @brand_name %> <%= Date.today.strftime("%B %Y") %> REPORT for <%= @branch %> BRANCH
</div>
<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START chart-->
    <div class="row">
      <div class="col-lg-12">
        <!-- START widget-->
        <div id="panelChart9" class="panel panel-default panel-demo">
          <div class="panel-heading">
            <div class="panel-title" style="text-align: center">
              <%= @brand_name %> sales Daily <%= Date.today.strftime("%B %Y") %> sales statistics
            </div>
          </div>
          <div class="panel-body">
            <div class="chart-spline-daily flot-chart"></div>
          </div>
        </div>
        <!-- END widget-->
      </div>
    </div>
  </div>
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-6 col-lg-offset-3">
        <div class="panel panel-warning">
          <div class="panel-heading" style="text-align: center">
            Total Revenue <%= Date.yesterday.strftime("%B %Y") %>
          </div>
          <div class="panel-body">
            <table id="table-revenue" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th><%= Date.yesterday.strftime('%b %y') %></th>
                  <th><%= Date.yesterday.last_month.strftime('%b %y') %></th>
                  <th>Change</th>
                  <th><%= Date.yesterday.last_year.strftime('%b %y') %></th>
                  <th>YoY</th>
                </tr>
              </thead>
              <tbody>
                <% @sales_revenue_this_month.each do |dp| %>
                <tr class="gradeX">
                  <td><%= currency(dp.val_1) %></td>
                  <td><%= currency(dp.val_2) %></td>
                  <td class="<%= coloring(dp.percentage) %>"><%= dp.percentage.nil? ? 0 : cpercent(dp.percentage) %></td>
                  <td><%= currency(dp.revenue) %></td>
                  <td class="<%= coloring(dp.y_percentage) %>"><%= dp.y_percentage.nil? ? 0 : cpercent(dp.y_percentage) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
    <!-- END content-->
  </div>
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            Sales Report From 2 days ago by Product
          </div>
          <div class="panel-body">
            <table id="datatable_product" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Product</th>
                  <th>Q_<%= 1.day.ago.to_date.strftime('%d') %></th>
                  <th>V_<%= 1.day.ago.to_date.strftime('%d') %></th>
                  <th>Q_<%= 2.day.ago.to_date.strftime('%d') %></th>
                  <th>V_<%= 2.day.ago.to_date.strftime('%d') %></th>
                </tr>
              </thead>
              <tbody>
                <% @sales_daily_product.each do |dp| %>
                <tr class="gradeX">
                  <td><%= dp.kodejenis %></td>
                  <td><%= dp.qty_1 %></td>
                  <td><%= currency(dp.val_1) %></td>
                  <td><%= dp.qty_2 %></td>
                  <td><%= currency(dp.val_2) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            Sales Report This Week(<%= Date.yesterday.beginning_of_week.day %>-<%= Date.yesterday.end_of_week.strftime('%d %B %y') %>) by Product
          </div>
          <div class="panel-body">
            <table id="datatable1" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Customer</th>
                  <th>Q_W(<%= 1.day.ago.to_date.cweek %>)</th>
                  <th>V_W(<%= 1.day.ago.to_date.cweek %>)</th>
                  <th>Q_W(<%= 1.day.ago.to_date.last_week.cweek %>)</th>
                  <th>V_W(<%= 1.day.ago.to_date.last_week.cweek %>)</th>
                  <th>WoW</th>
                </tr>
              </thead>
              <tbody>
                <% @this_week_product.each do |dp| %>
                <tr class="gradeX">
                  <td><%= dp.kodejenis %></td>
                  <td><%= dp.qty_1 %></td>
                  <td><%= currency(dp.val_1) %></td>
                  <td><%= dp.qty_2 %></td>
                  <td><%= currency(dp.val_2) %></td>
                  <td class="<%= coloring(dp.percentage) %>"><%= dp.percentage.nil? ? 0 : cpercent(dp.percentage) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
    <!-- END content-->
  </div>
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            Sales Report This Month by Product
          </div>
          <div class="panel-body">
            <table id="table_without_filter1" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Product</th>
                  <th><%= Date.yesterday.strftime('%b %y') %></th>
                  <th><%= Date.yesterday.strftime('%b %y') %></th>
                  <th>MoM</th>
                  <th>Target</th>
                  <th>RoT</th>
                </tr>
              </thead>
              <tbody>
                <% @this_month_product.each do |dp| %>
                <tr class="gradeX">
                  <td><%= dp.kodejenis %></td>
                  <td><%= dp.qty_1 %></td>
                  <td><%= currency(dp.val_1) %></td>
                  <td class="<%= coloring(dp.percentage) %>"><%= dp.percentage.nil? ? 0 : cpercent(dp.percentage) %></td>
                  <td class="<%= coloring_target(dp.target) %>"><%= dp.target.nil? ? 0 : cpercent(dp.target) %></td>
                  <td><%= dp.rot.nil? ? 0 : (dp.rot < 0 ? 0 : dp.rot) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            Sales Report This Month by Salesman
          </div>
          <div class="panel-body">
            <table id="table_without_filter2" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Salesman</th>
                  <th><%= Date.yesterday.strftime('%b %y') %></th>
                  <th><%= Date.yesterday.strftime('%b %y') %></th>
                  <th>MoM</th>
                  <th>Target</th>
                  <th>RoT</th>
                </tr>
              </thead>
              <tbody>
                <% @this_month_salesman.each do |dp| %>
                <tr class="gradeX">
                  <td><%= dp.salesman %></td>
                  <td><%= dp.qty_1 %></td>
                  <td><%= currency(dp.val_1) %></td>
                  <td class="<%= coloring(dp.percentage) %>"><%= dp.percentage.nil? ? 0 : cpercent(dp.percentage) %></td>
                  <td class="<%= coloring_target(dp.target) %>"><%= dp.target.nil? ? 0 : cpercent(dp.target) %></td>
                  <td><%= dp.rot.nil? ? 0 : (dp.rot < 0 ? 0 : dp.rot) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- END content-->
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            Sales Report This Month by Customer
          </div>
          <div class="panel-body">
            <table id="datatable_customer" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Customer</th>
                  <th>KM</th>
                  <th>DV</th>
                  <th>HB</th>
                  <th>SA</th>
                  <th>SB</th>
                  <th>Total</th>
                  <th>MoM</th>
                </tr>
              </thead>
              <tbody>
                <% @this_month_customer.each do |dp| %>
                <tr class="gradeX">
                  <td><%= dp.customer %></td>
                  <td><%= dp.km %></td>
                  <td><%= dp.dv %></td>
                  <td><%= dp.hb %></td>
                  <td><%= dp.sa %></td>
                  <td><%= dp.sb %></td>
                  <td><%= currency(dp.total_1) %></td>
                  <td class="<%= coloring(dp.percentage) %>"><%= dp.percentage.nil? ? 0 : cpercent(dp.percentage) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            Sales Report This Month by City
          </div>
          <div class="panel-body">
            <table id="datatable_city" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>City</th>
                  <th>KM</th>
                  <th>DV</th>
                  <th>HB</th>
                  <th>SA</th>
                  <th>SB</th>
                  <th>Total</th>
                  <th>MoM</th>
                </tr>
              </thead>
              <tbody>
                <% @this_month_city.each do |dp| %>
                <tr class="gradeX">
                  <td><%= dp.kota %></td>
                  <td><%= dp.km %></td>
                  <td><%= dp.dv %></td>
                  <td><%= dp.hb %></td>
                  <td><%= dp.sa %></td>
                  <td><%= dp.sb %></td>
                  <td><%= currency(dp.total_1) %></td>
                  <td class="<%= coloring(dp.percentage) %>"><%= dp.percentage.nil? ? 0 : cpercent(dp.percentage) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <!-- END content-->
    </div>
  </div>
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            Sales Report This Month by Article
          </div>
          <div class="panel-body">
            <table id="stock-table" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Article</th>
                  <th>Size</th>
                  <th><%= Date.yesterday.strftime('%b %y') %></th>
                  <th><%= Date.yesterday.strftime('%b %y') %></th>
                  <th><%= Date.yesterday.last_month.strftime('%b %y') %></th>
                  <th><%= Date.yesterday.last_month.strftime('%b %y') %></th>
                  <th>MoM</th>
                </tr>
              </thead>
              <tbody>
                <% @this_month_article.each do |dp| %>
                <tr class="gradeX">
                  <td><%= dp.namaartikel %></td>
                  <td><%= dp.lebar %></td>
                  <th><%= dp.qty_1 %></th>
                  <th><%= currency(dp.val_1) %></th>
                  <th><%= dp.qty_2 %></th>
                  <th><%= currency(dp.val_2) %></th>
                  <td class="<%= coloring(dp.percentage) %>"><%= dp.percentage.nil? ? 0 : cpercent(dp.percentage) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
    <!-- END content-->
  </div>
</div>
