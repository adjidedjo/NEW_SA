<div class="content-heading">
  OVERVIEW DIRECT SALES NASIONAL <%= @brand_name %> From 4 Weeks Ago to Last Week
</div>
<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START chart-->
    <div class="row">
      <div class="col-lg-12">
        <!-- START widget-->
        <div id="panelChart9" class="panel panel-default panel-demo">
          <div class="panel-heading">
            <div class="panel-title" style="text-align: center">
              Nasional DIRECT <%= @brand_name %> Weekly sales statistics <br /> (x10.000.000)
            </div>
          </div>
          <div class="panel-body">
            <div class="chart-spline-weeknas flot-chart"></div>
          </div>
        </div>
        <!-- END widget-->
      </div>
    </div>
  </div>
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-12">
        <div class="panel panel-info">
          <div class="panel-heading">
            Sales Report Last Week <%= 1.week.ago.to_date.cweek %> (<%= 1.week.ago.beginning_of_week.day %>-<%= 1.week.ago.end_of_week.strftime('%d %B %y') %>) by Branch
          </div>
          <div class="panel-body">
            <table id="datatable1" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Branch</th>
                  <th>Qty Week <%= 4.weeks.ago.to_date.cweek %></th>
                  <th>Val Week <%= 4.weeks.ago.to_date.cweek %></th>
                  <th>Qty Week <%= 3.weeks.ago.to_date.cweek %></th>
                  <th>Val Week <%= 3.weeks.ago.to_date.cweek %></th>
                  <th>Change <%= 3.weeks.ago.to_date.cweek %>:<%= 4.weeks.ago.to_date.cweek %></th>
                  <th>Qty Week <%= 2.weeks.ago.to_date.cweek %></th>
                  <th>Val Week <%= 2.weeks.ago.to_date.cweek %></th>
                  <th>Change <%= 2.weeks.ago.to_date.cweek %>:<%= 3.weeks.ago.to_date.cweek %></th>
                  <th>Qty Week <%= 1.weeks.ago.to_date.cweek %></th>
                  <th>Val Week <%= 1.weeks.ago.to_date.cweek %></th>
                  <th>Change <%= 1.weeks.ago.to_date.cweek %>:<%= 2.weeks.ago.to_date.cweek %></th>
                </tr>
              </thead>
              <tbody>
                <% @ret_nas_weekbranch.each do |dp| %>
                <tr class="gradeX">
                  <td><%= dp.cabang.gsub('Cabang', '') %></td>
                  <td><%= dp.qty_4 %></td>
                  <td><%= currency(dp.val_4) %></td>
                  <td><%= dp.qty_3 %></td>
                  <td><%= currency(dp.val_3) %></td>
                  <td class="<%= coloring(dp.chg_3) %>"><%= dp.chg_3.nil? ? 0 : cpercent(dp.chg_3.abs) %></td>
                  <td><%= dp.qty_2 %></td>
                  <td><%= currency(dp.val_2) %></td>
                  <td class="<%= coloring(dp.chg_2) %>"><%= dp.chg_2.nil? ? 0 : cpercent(dp.chg_2.abs) %></td>
                  <td><%= dp.qty_1 %></td>
                  <td><%= currency(dp.val_1) %></td>
                  <td class="<%= coloring(dp.chg_1) %>"><%= dp.chg_1.nil? ? 0 : cpercent(dp.chg_1.abs) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
    <!-- END content-->
  </div>
</div>