<div class="content-heading">
  Dashboard Serenity WEEK(<%= Date.today.cweek %>) REPORT for PALEMBANG BRANCH
  <small>FROM <%= Date.today.beginning_of_week.strftime("%d %B %Y") %> - <%= Date.today.end_of_week.strftime("%d %B %Y") %></small>
</div>
<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <div class="col-lg-9">
    <!-- START chart-->
    <div class="row">
      <div class="col-lg-12">
        <!-- START widget-->
        <div id="panelChart9" class="panel panel-default panel-demo">
          <div class="panel-heading">
            <a href="#" data-tool="panel-refresh" data-toggle="tooltip" title="Refresh Panel" class="pull-right"> <em class="fa fa-refresh"></em> </a>
            <a href="#" data-tool="panel-collapse" data-toggle="tooltip" title="Collapse Panel" class="pull-right"> <em class="fa fa-minus"></em> </a>
            <div class="panel-title">
              Serenity Week(<%= Date.today.cweek %>) sales statistics
            </div>
          </div>
          <div class="panel-body">
            <div class="chart-spline-weekly flot-chart"></div>
          </div>
        </div>
        <!-- END widget-->
      </div>
    </div>
    <!-- END chart-->
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-default">
          <div class="panel-heading">
            Serenity weekly report
          </div>
          <div class="panel-body">
            <table id="datatable1" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Minggu ke</th>
                  <th>Tanggal</th>
                  <th class="sort-numeric">Jumlah</th>
                  <th class="sort-alpha">Total</th>
                </tr>
              </thead>
              <tbody>
                <% @all_week_serenity.each do |cs| %>
                <tr class="gradeX">
                  <td><%= cs.tanggalsj.cweek %></td>
                  <td><%= cs.tanggalsj.beginning_of_week.strftime("%d/%m/%Y") %> - <%= cs.tanggalsj.end_of_week.strftime("%d/%m/%Y") %></td>
                  <td><%= cs.jumlah %></td>
                  <td><%= currency(cs.price) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="col-lg-6">
        <div class="panel panel-default">
          <div class="panel-heading">
            Serenity product weekly report
          </div>
          <div class="panel-body">
            <table id="datatable_product" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Produk</th>
                  <th class="sort-numeric">Jumlah</th>
                  <th class="sort-alpha">Total</th>
                </tr>
              </thead>
              <tbody>
                <% @product_summary.each do |cs| %>
                <tr class="gradeX">
                  <td><%= cs.jenisbrg %></td>
                  <td><%= cs.jumlah %></td>
                  <td><%= currency(cs.price) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="col-lg-12">
        <div class="panel panel-default">
          <div class="panel-heading">
            Serenity customer sales Week(<%= Date.today.cweek %>)
          </div>
          <div class="panel-body">
            <table id="datatable_customer" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Customer</th>
                  <th>Salesman</th>
                  <th class="sort-numeric">Jumlah</th>
                  <th class="sort-alpha">Total Harga</th>
                </tr>
              </thead>
              <tbody>
                <% @customer_summary.each do |cs| %>
                <tr class="gradeX">
                  <td><%= cs.customer %></td>
                  <td><%= cs.salesman %></td>
                  <td><%= cs.ordered %></td>
                  <td><%= currency(cs.price) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- END dashboard main content-->
  <!-- START dashboard sidebar-->
  <aside class="col-lg-3">
    <!-- START loader widget-->
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="panel-title">
          Serenity Channel This Week
        </div>
      </div>
      <!-- START list group-->
      <div class="list-group">
        <!-- START list group item-->
        <div class="list-group-item">
          <div class="media-box">
            <div class="pull-left">
              <span class="fa-stack"> <em class="fa fa-circle fa-stack-2x text-success"></em> <em class="fa fa-clock-o fa-stack-1x fa-inverse text-white"></em> </span>
            </div>
            <div class="media-box-body clearfix">
              <small class="text-muted pull-right ml"><%= @retail.empty? ? 0 : @retail.first.jumlah %></small>
              <div class="media-box-heading">
                RETAIL
              </div>
              <p class="m0">
                <small><a href="#">Details</a> </small>
              </p>
            </div>
          </div>
        </div>
        <!-- END list group item-->
        <!-- START list group item-->
        <div class="list-group-item">
          <div class="media-box">
            <div class="pull-left">
              <span class="fa-stack"> <em class="fa fa-circle fa-stack-2x text-success"></em> <em class="fa fa-clock-o fa-stack-1x fa-inverse text-white"></em> </span>
            </div>
            <div class="media-box-body clearfix">
              <small class="text-muted pull-right ml"><%= (@direct.empty? ? 0 : @direct.first.jumlah) +  (@showroom.empty? ? 0 : @showroom.first.jumlah)%></small>
              <div class="media-box-heading">
                DIRECT
              </div>
              <p class="m0">
                <small><a href="#">Details</a> </small>
              </p>
            </div>
          </div>
        </div>
        <!-- END list group item-->
        <!-- START list group item-->
        <div class="list-group-item">
          <div class="media-box">
            <div class="pull-left">
              <span class="fa-stack"> <em class="fa fa-circle fa-stack-2x text-success"></em> <em class="fa fa-clock-o fa-stack-1x fa-inverse text-white"></em> </span>
            </div>
            <div class="media-box-body clearfix">
              <small class="text-muted pull-right ml"><%= @modern.empty? ? 0 : @modern.first.jumlah %></small>
              <div class="media-box-heading">
                MODERN
              </div>
              <p class="m0">
                <a href="#">Details</a>
              </p>
            </div>
          </div>
        </div>
        <!-- END list group item-->
        <!-- START list group item-->
        <div class="list-group-item">
          <div class="media-box">
            <div class="pull-left">
              <span class="fa-stack"> <em class="fa fa-circle fa-stack-2x text-success"></em> <em class="fa fa-clock-o fa-stack-1x fa-inverse text-white"></em> </span>
            </div>
            <div class="media-box-body clearfix">
              <small class="text-muted pull-right ml"><%= @project.empty? ? 0 : @project.first.jumlah %></small>
              <div class="media-box-heading">
                PROJECT
              </div>
              <p class="m0">
                <a href="#">Details</a>
              </p>
            </div>
          </div>
        </div>
        <!-- END list group item-->
        <!-- START list group item-->
        <div class="list-group-item">
          <div class="media-box">
            <div class="pull-left">
              <span class="fa-stack"> <em class="fa fa-circle fa-stack-2x text-success"></em> <em class="fa fa-clock-o fa-stack-1x fa-inverse text-white"></em> </span>
            </div>
            <div class="media-box-body clearfix">
              <small class="text-muted pull-right ml"><%= @uncategorized.empty? ? 0 : @uncategorized.first.jumlah %></small>
              <div class="media-box-heading">
                UNCATEGORIZED
              </div>
              <p class="m0">
                <a href="#">Details</a>
              </p>
            </div>
          </div>
        </div>
        <!-- END list group item-->
      </div>
      <!-- END list group-->
      <!-- START panel footer-->
      <div class="panel-footer clearfix">
        <a href="#" class="pull-left"> <small>Load more</small> </a>
      </div>

    </div>
    <!-- END loader widget-->
    <!-- START messages and activity-->
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="panel-title">
          Serenity Most Items Delivered This Week
        </div>
      </div>
      <!-- START list group-->
      <div class="list-group">
        <% @most_item.each do |most| %>
        <!-- START list group item-->
        <div class="list-group-item">
          <div class="media-box">
            <div class="pull-left">
              <span class="fa-stack"> <em class="fa fa-circle fa-stack-2x text-purple"></em> <em class="fa fa-cloud-upload fa-stack-1x fa-inverse text-white"></em> </span>
            </div>
            <div class="media-box-body clearfix">
              <small class="text-muted pull-right ml"><%= most.ordered %></small>
              <div class="media-box-heading">
                <%= most.namaartikel %>
              </div>
            </div>
          </div>
        </div>
        <!-- END list group item-->
        <% end %>
      </div>
      <!-- END list group-->
      <!-- START panel footer-->
      <div class="panel-footer clearfix">
        <a href="#" class="pull-left"> <small>Load more</small> </a>
      </div>

    </div>
    <!-- END messages and activity-->
  </aside>
  <!-- END dashboard sidebar-->
</div>