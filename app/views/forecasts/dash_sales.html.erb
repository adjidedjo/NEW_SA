<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-8 col-lg-offset-2">
        <div class="panel panel-warning">
          <div class="panel-heading" style="text-align: center">
            SCORE CARD (FORECAST - REALISASI) Minggu <%= Date.today.cweek %> Tahun <%= Date.today.year%>
          </div>
          <div class="panel-body">
            <table id="datatable_customer" class="table table-striped table-hover cell-border">
              <thead>
                <tr>
                  <th>Brand</th>
                  <th>Nama Barang</th>
                  <th>000</th>
                  <th>090</th>
                  <th>100</th>
                  <th>120</th>
                  <th>140</th>
                  <th>160</th>
                  <th>180</th>
                  <th>200</th>
                </tr>
              </thead>
              <tbody>
                <% @sales_month.each do |dp| %>
                <tr class="gradeX">
                  <td><%= dp.brand %></td>
                  <td><%= dp.segment2_name + ' ' + dp.segment3_name%></td>
                  <td><%= dp.satu %></td>
                  <td><%= dp.dua %></td>
                  <td><%= dp.tiga %></td>
                  <td><%= dp.empat %></td>
                  <td><%= dp.lima %></td>
                  <td><%= dp.enam %></td>
                  <td><%= dp.tujuh %></td>
                  <td><%= dp.delapan %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
    <!-- END content-->
  </div>
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
    </div>
    <!-- END content-->
  </div>
</div>