<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-8 col-lg-offset-2">
        <div class="panel panel-warning">
          <div class="panel-heading" style="text-align: center">
            Total Revenue <%= Date.yesterday.beginning_of_month.strftime("%d") %> - <%= Date.yesterday.strftime("%d %B %Y") %>
          </div>
          <div class="panel-body">
            <table id="table-revenue-salesman" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Nama</th>
                  <th>Brand</th>
                  <th>Quantity</th>
                  <th>Amount</th>
                  <th>Target Week <%= Date.today.cweek %></th>
                </tr>
              </thead>
              <tbody>
                <% @sales_month.each do |dp| %>
                <tr class="gradeX">
                  <td><%= dp.salesmen_desc %></td>
                  <td><%= dp.brand %></td>
                  <td><%= dp.qty %></td>
                  <td><%= currency(dp.amount) %></td>
                  <td><%= dp.total %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
    <!-- END content-->
  </div>
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
    </div>
    <!-- END content-->
  </div>
</div>