<div class="panel panel-default">
  <div class="panel-heading">
    PBJM FORM CABANG
  </div>
  <div class="panel-body">
    <%= form_tag(forecasts_report_pbjm_cabang_path, {method: :get, :class => "form-inline"}) do %>
    <div class="form-group">
      From:
    </div>
    <div id="dateffore3" class="input-group date" style="size: 10px;">
      <%= text_field_tag "start_date", (params[:start_date].present? ? params[:start_date].to_date.strftime("%F") : Date.today), class: 'form-control' %>
      <span class="input-group-addon"> <span class="fa fa-calendar"></span> </span>
    </div>
    <div class="form-group">
      To:
    </div>
    <div id="datetfore3" class="input-group date" style="size: 10px;">
      <%= text_field_tag "end_date", (params[:end_date].present? ? params[:end_date].to_date.strftime("%F") : Date.today), class: 'form-control' %>
      <span class="input-group-addon"> <span class="fa fa-calendar"></span> </span>
    </div>
    <div class="form-group">
      Branch:
    </div>
    <div id="branch_text" class="input-group" style="size: 10px;">
      <%= text_field_tag "branch", nil, placeholder: 'Contoh: 18011', class: 'form-control' %>
      <span class="input-group-addon"> </span>
    </div>
    <%= button_tag 'Export', value: 'xlsx', name: 'format', class: "btn btn-info" %>
    <% end %>
  </div>
</div>