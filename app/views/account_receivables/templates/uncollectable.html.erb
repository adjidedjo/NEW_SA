<div class="content-heading">
  ACCOUNT RECEIVABLE AGING REPORT for <%= @brand_name %> BRAND AT <%= @branch_name %> BRANCH
</div>

<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            A/R Aging With Due Date are <%= 1.month.ago.strftime('%B %Y') %>
          </div>
          <div class="panel-body">
            <table id="held_orders" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Customer</th>
                  <th>Salesman</th>
                  <th>Due Date</th>
                  <th>Open Amount</th>
                </tr>
              </thead>
              <tbody>
                <% @u_ar10.each do |ar| %>
                <tr class="gradeX">
                  <td><%= ar.customer %></td>
                  <td><%= ar.salesman %></td>
                  <td><%= ar.due_date.nil? ? '' : ar.due_date.strftime(('%B %y')) %></td>
                  <td><%= currency(ar.open_amount) %></td>
                <% end %>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="col-lg-6">
        <div class="panel panel-warning">
          <div class="panel-heading">
            A/R Aging With Due Date are more than <%= 1.month.ago.strftime('%B %Y') %>
          </div>
          <div class="panel-body">
            <table id="table_without_filter1" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Customer</th>
                  <th>Salesman</th>
                  <th>Due Date</th>
                  <th>Open Amount</th>
                </tr>
              </thead>
              <tbody>
                <% @u_ar20.each do |ar| %>
                <tr class="gradeX">
                  <td><%= ar.customer %></td>
                  <td><%= ar.salesman %></td>
                  <td><%= ar.due_date.nil? ? '' : ar.due_date.strftime(('%B %y')) %></td>
                  <td><%= currency(ar.open_amount) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
    <!-- END content-->
  </div>
</div>
