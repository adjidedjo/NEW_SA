<div class="content-heading">
  ACCOUNT RECEIVABLE COLLECTABLE REPORT for <%= @brand_name %> BRAND AT <%= @branch_name %> BRANCH
</div>
<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-12">
        <div class="panel panel-info">
          <div class="panel-heading">
            A/R Collectable <b><%= 5.days.ago.strftime('%B %Y')%></b>
          </div>
          <div class="panel-body">
            <table id="held_orders" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Customer</th>
                  <th>Salesman</th>
                  <th>Open Amount</th>
                  <th>Bad Debt</th>
                </tr>
              </thead>
              <tbody>
                <% @cus_ar.each do |ar| %>
                <tr class="gradeX">
                  <td><%= ar.customer %></td>
                  <td><%= ar.salesman %></td>
                  <td><%= currency(ar.open_amount) %></td>
                  <td><%= currency(ar.bad_debt) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            A/R Collectable <b><%= 5.days.ago.strftime('%B %Y')%></b>
          </div>
          <div class="panel-body">
            <table id="datatable_product" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Description</th>
                  <th>Amount</th>
                </tr>
              </thead>
              <tbody>
                <% @u_ar.each do |ar| %>
                <tr class="gradeX">
                  <td><%= ar.description %></td>
                  <td><%= currency(ar.gross_amount) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="col-lg-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            A/R Collectable Percentage <b><%= 5.days.ago.strftime('%B %Y')%></b>
          </div>
          <div class="panel-body">
            <table id="datatable1" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Description</th>
                  <th>Percentage</th>
                </tr>
              </thead>
              <tbody>
                <% @u_percentage.each do |ar| %>
                <tr class="gradeX">
                  <td><%= ar.description %></td>
                  <td><%= ar.percent %>%</td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>