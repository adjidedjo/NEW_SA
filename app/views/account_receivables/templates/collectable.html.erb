<div class="content-heading">
  ACCOUNT RECEIVABLE COLLECTABLE REPORT for <%= @brand_name %> BRAND AT <%= @branch_name %> BRANCH
</div>
<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-6">
        <div class="panel panel-success">
          <div class="panel-heading">
            A/R Collectable
          </div>
          <div class="panel-body">
            <table id="datatable_product" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Description</th>
                  <th>Amount</th>
                </tr>
              </thead>
              <tbody>
                <% @u_ar.each do |ar| %>
                <tr class="gradeX">
                  <td><%= ar.description %></td>
                  <td><%= currency(ar.gross_amount) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>