<div class="content-heading">
  AGING STOCK <%= @state %> <%= @brand %> <%= Date.today.strftime("%d %B %Y") %> REPORT for <%= @branch %>
</div>
<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <div class="row">
      <div class="col-lg-12">
        <div class="panel panel-info">
          <div class="panel-heading" style="text-align: center">
            Stock Aging
          </div>
          <div class="panel-body">
            <table id="table-aging-stock" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Branch</th>
                  <th>Brand</th>
                  <th>Type</th>
                  <th>1-2 M</th>
                  <th>2-4 M</th>
                  <th>4-6 M</th>
                  <th>6-12 M</th>
                  <th>12-24 M</th>
                  <th>24 M</th>
                </tr>
              </thead>
              <tfoot>
                <tr>
                  <th>Type</th>
                  <th>Type</th>
                </tr>
              </tfoot>
              <tbody>
                <% @age_stock.each do |cs| %>
                <tr class="gradeX">
                  <td><%= cs.branch_plan_desc %></td>
                  <td><%= cs.brand %></td>
                  <td><%= cs.grouping %></td>
                  <td><%= cs.satu.to_i %></td>
                  <td><%= cs.dua.to_i %></td>
                  <td><%= cs.empat.to_i %></td>
                  <td><%= cs.enam.to_i %></td>
                  <td><%= cs.duabelas.to_i %></td>
                  <td><%= cs.duaempat.to_i %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
          <div class="panel-footer">
            <ul>
              <li>UNTUK DETAIL BARANG, SILAHKAN MENGGUNAKAN JDE %></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- END dashboard main content-->
</div>
